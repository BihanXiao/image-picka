{
  "title": "Image Picka",
  "name": "image-picka",
  "version": "0.1.0",
  "description": "An image picker written in webextensions",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "npm run lint && web-ext lint",
    "start": "web-ext run",
    "preversion": "npm test",
    "version": "sync-version manifest.json && git add .",
    "build": "web-ext build --ignore-files **/*.log package.json package-lock.json --overwrite-dest",
    "postversion": "npm run build && git push --follow-tags"
  },
  "devDependencies": {
    "npm-build-tools": "^2.2.5",
    "sync-version": "^1.0.1",
    "web-ext": "^2.0.0"
  }
}
